      "import graphlab\n",
      "sf = graphlab.SFrame(data='http://graphlab.com/files/datasets/freebase_performances.csv')\n",
      "print sf\n",
      "g = graphlab.Graph()\n",
      "g = g.add_edges(sf, 'actor_name', 'film_name')\n",
      "pr = graphlab.pagerank.create(g)\n",
      "pr.get('pagerank').topk(column_name='pagerank')"
     ],
